{
  "bindings": [
    {
      "authLevel": "function",
      "name": "req",
      "type": "httpTrigger",
      "methods": [ "get" ],
      "route": "counter/{countername}",
      "direction": "in"
    },
    {
      "name": "res",
      "type": "http",
      "direction": "out"
    },
    {
      "type": "table",
      "name": "counters",
      "tableName": "apicountertablelocal",
      "connection": "AzureWebJobsStorage",
      "direction": "in",
      "partitionKey": "items"
    }
  ],
  "disabled": false
}