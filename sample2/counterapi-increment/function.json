{
  "bindings": [
    {
      "authLevel": "function",
      "name": "req",
      "type": "httpTrigger",
      "methods": [ "post" ],
      "route": "counterapi?add=int",
      "direction": "in"
    },
    {
      "name": "res",
      "type": "http",
      "direction": "out"
    },
    {
      "type": "table",
      "name": "counter",
      "tableName": "apicountertablelocal",
      "connection": "AzureWebJobsStorage",
      "direction": "in",
      "partitionKey": "items",
      "rowKey": "Counter"
    }
  ],
  "disabled": false
}