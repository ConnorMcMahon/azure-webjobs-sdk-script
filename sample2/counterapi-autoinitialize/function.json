{
  "bindings": [
    {
      "authLevel": "function",
      "name": "req",
      "type": "httpTrigger",
      "direction": "in"
    },
    {
      "name": "res",
      "type": "http",
      "direction": "out"
    },
    {
      "type": "table",
      "name": "counterIn",
      "tableName": "apicountertablelocal",
      "connection": "AzureWebJobsStorage",
      "partitionKey": "items",
      "direction": "in"
    },
    {
      "type": "table",
      "name": "counterOut",
      "tableName": "apicountertablelocal",
      "partitionKey": "items",
      "rowKey": "%rand-guid%",
      "connection": "AzureWebJobsStorage",
      "direction": "out"
    }
  ],
  "disabled": false
}